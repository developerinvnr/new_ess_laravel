	<Script>
		$(document).ready(function() {
		$('.oldkrabtn').click(function() {
			$('#oldkrabox').toggle();
		});
		$('.oldkraclose').click(function() {
			$('#oldkrabox').toggle();
		});
		$('.mykra').click(function() {
			$('#mykrabox').show();
			$('#mykraeditbox').hide();
		});
		$('.mykraedit').click(function() {
			$('#mykraeditbox').show();
			$('#mykrabox').hide();
		});
		$('.subkrabtn').click(function() {
			$('#subkrabtnbox').show();
		});
		
	});
		</Script>
